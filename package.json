{
  "name": "al-default-project-settings",
  "displayName": "ALdefaultProjectSettings",
  "description": "A settings extension for Microsoft Dynamics 365 Business Central development. Generates a standard settings.json for AL projects",
  "version": "0.1.5",
  "publisher": "mc-wusel",
  "engines": {
    "vscode": "^1.87.0"
  },
  "author": {
    "name": "mc-wusel",
    "url": "",
    "email": ""
  },
  "galleryBanner": {
    "color": "#0060A0",
    "theme": "dark"
  },
  "icon": "Assets/Graphics/JPG/pittbull.jpg",
  "repository": {
    "type": "git",
    "url": "https://github.com/mc-wusel/ALdefaultProjectSettings"
  },
  "bugs": {
    "url": "https://github.com/mc-wusel/ALdefaultProjectSettings/issues"
  },
  "homepage": "",
  "preview": false,
  "categories": [
    "Programming Languages",
    "Snippets",
    "Other"
  ],
  "keywords": [
    "al"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mc.go",
        "title": "Go!",
        "category": "MC"
      },
      {
        "command": "mc.right",
        "title": "Explorer Right",
        "category": "MC"
      },
      {
        "command": "mc.left",
        "title": "Explorer Left",
        "category": "MC"
      },
      {
        "command": "mc.toggleAppSourceCop",
        "title": "Toggle AppSourceCop",
        "category": "MC"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.87.0",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "ts-mockito": "^2.6.1",
    "typescript": "^5.3.3"
  }
}
